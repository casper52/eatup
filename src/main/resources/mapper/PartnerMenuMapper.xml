  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ga.eatup.partner.mapper.PartnerMenuMapper">

	<select id="getMenu"
		resultType="ga.eatup.partner.domain.PartnerMenuVO">
		select * from
		partner_store_menu
		where sno = #{sno}
	</select>



</mapper>

 